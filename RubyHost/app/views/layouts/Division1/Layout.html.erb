<!DOCTYPE html>
<html xmlns = "http://www.w3.org/1999/xhtml">
	<%= render "/division1/Header" %>
	<body>
		<div id="idGeneralLayout">
			<Header id="idHeaderRow">
				<div id="idHeaderRowCenter" colspan="3">
					<%= render "/Universal/Logo" %>
				</div>
			</Header>
			<Nav id="idNavigationRow">
				<div id="idNavigationBar" colspan="3">
					<%= render "/Universal/NavBar" %>
				</div>
			</Nav>				
			<div id="idCenterRow">
				<div id="idCenterRowLeft">
					<%= render "/Universal/NavigationHeader" %>
					<%= render "/division1/Navigation" %>
				</div>
				<div id="idCenterRowMain">
					<%= yield %>
				</div>
				<div id="idCenterRowRight">
					<%= render "/Universal/InfoHeader" %>
					<%= render "/Universal/InfoLanguage" %>
					<%= yield :versions %>
				</div>
			</div>
			<Footer id="idFooterRow">
				<div id="idFooterMain" colspan="3">
					<%= render "/Universal/Footer" %>
					<%= render "/Universal/Management" %>
				</div>
			</Footer>
		</div>
	</body>
</html>